{
    "extends": "../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            ["sf", ""],
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "sf",
            "kebab-case"
        ],
        "component-class-suffix": [
          true,
          "Component",
          "Widget"
        ]
    }
}
